package class1;

public  class car extends vehicle {
	public  void engine(double x)
	{
		if((x>0)||(x<200))
		{
			System.out.println(x+("is engine of mehran car"));
		}
		else if((x>200)||(x<400))
		{
			System.out.println(x+("is engine of cultus car"));
		}
		else if((x>400)||(x<600))
		{
			System.out.println(x+("is engine of civic car "));
		}
		else
		{
			System.out.println(x+("is not engine of car"));
		}
	}
}
